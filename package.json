{
  "name": "simplelogin-client",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "tsx src/index.ts",
    "build": "tsx scripts/build.ts",
    "rebuild": "tsx scripts/rebuild.ts",
    "dev": "npm-watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "dependency-update": "npm-check-updates -u --filterVersion \"/^[~^<>]| - |\\.x$/\" --deep && npm install",
    "prepare": "husky install"
  },
  "watch": {
    "build": {
      "patterns": "oas",
      "extensions": "yml,yaml"
    }
  },
  "lint-staged": {
    "src/*.ts": "eslint --cache --fix",
    "*.{ts,json,yml,yaml,html,md}": "prettier --write"
  },
  "engines": {
    "node": "18",
    "npm": "9"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "18.*",
    "typescript": "^4.9.4",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "eslint": "^8.31.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "@moia-oss/eslint-prettier-typescript-config": "^0.11.41",
    "husky": "^8.0.3",
    "lint-staged": "^13",
    "npm-check-updates": "^16.6.2",
    "prettier": "^2.8.2",
    "boats": "^3.4.0",
    "globby": "^13.1.3",
    "npm-watch": "^0.11.0",
    "redoc-cli": "^0.13.20",
    "tsx": "^3.12.3",
    "typedoc": "^0.23.24",
    "zx": "^7.1.1"
  }
}
